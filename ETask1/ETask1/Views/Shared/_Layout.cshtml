<!DOCTYPE html>
<html lang="en">
    <head>
      <meta charset="utf-8" />
        <title>@ViewBag.Title - My ASP.NET MVC Application</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
		
		
        <script type='text/javascript' src="@Url.Content("~/Scripts/js/jquery-1.6.2.min.js")"></script>
         
		<script type='text/javascript' src="@Url.Content("~/Scripts/js/menu_jquery.js")"></script> <script type="text/javascript" src="@Url.Content("~/Scripts/js/jquery.datepick.js")"></script>  
        
          
		<script type='text/javascript' src="@Url.Content("~/Scripts/js/Script.js")"></script>
		


        @Styles.Render("~/Content/css")
        
    </head>
    <body>
       <div id="wrapper">

				<div id="header">
		
					<div class="logo"><img src="@Url.Content("~/Content/images/logo.png")"/></div>
			
				</div>
				
				<div id='cssmenu'>
	@RenderSection("menu",required:false)
				
			
				</div>

				<div id="container">
                @RenderBody()
            </div>
        
		
				<div id="footer">
	
					<p>Copyright @DateTime.Now.Year For problems with this website,please contact 9495655205</p><a href="privacy policy">Privacy Policy</a>
	
				</div>
	
			</div>

        
        @RenderSection("scripts", required: false)
    </body>
</html>
